   "source": [
    "#reverse string using loop\n",
    "\n",
    "def reverse(s):\n",
    "    str = \" \"\n",
    "    for i in s:\n",
    "        str=i + str\n",
    "        return str\n",
    "\n",
    "s=\"Novo Nordisk\"\n",
    "print(\"The original string is : \", end=\"\")\n",
    "print(s)\n",
    "\n",
    "print(\"The reversed string(using loops) is : \", end=\"\")\n",
    "print(reverse(s))\n",
    "\n",
    "\n"
   ]
